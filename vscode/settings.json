{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "[python]": {
        "editor.defaultFormatter": null
    },
    "editor.rulers": [80, 120],
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--skip-string-normalization"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "none",
    "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "material-icon-theme",
    "redhat.telemetry.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "vim.cursorStylePerMode.insert": "block",
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.leader": " ",
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.vimrc",
    "editor.lineNumbers": "relative",
    "vim.highlightedyank.enable": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.mac": "$HOME/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    }
}
